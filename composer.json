    "typo3/cms-fluid": "^9.5",
    "typo3/cms-frontend": "^9.5",
    "typo3/cms-indexed-search": "^9.5",
    "typo3/cms-install": "^9.5",
    "typo3/cms-lang": "^9.5",
    "typo3/cms-linkvalidator": "^9.5",
